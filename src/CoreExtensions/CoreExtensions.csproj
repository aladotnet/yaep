<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
	  <TargetFrameworks>netstandard2.1;net7.0</TargetFrameworks>
	  <LangVersion>11</LangVersion>
	  <ImplicitUsings>enable</ImplicitUsings>
	  <Description>YAEP Yet another extensions package is a collection of extension methods</Description>
	  <Authors>Ahmed Layachi</Authors>
	  <Company></Company>
	  <Copyright>Copyright $([System.DateTime]::Now.Year) Ahmed Layachi</Copyright>
	  <RepositoryUrl>https://github.com/aladotnet/yaep</RepositoryUrl>
	  <RepositoryType>git</RepositoryType>
	  <OutputType>Library</OutputType>
	  <IsPackable>true</IsPackable>
	  <LangVersion>10.0</LangVersion>
	  <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
	  <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
	  <PackageId>YAEP</PackageId>
	  <PackageProjectUrl>https://github.com/aladotnet/yaep</PackageProjectUrl>
	  <PackageIcon>YAEP.png</PackageIcon>
	  <PackageLicenseExpression>MIT</PackageLicenseExpression>
	  <PackageTags>c#;dotnet;extensions;utils</PackageTags>
	  <GenerateDocumentationFile>true</GenerateDocumentationFile>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(TargetFramework)|$(Platform)'=='Debug|netstandard2.1|AnyCPU'">
    <DebugType>portable</DebugType>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(TargetFramework)|$(Platform)'=='Release|netstandard2.1|AnyCPU'">
    <DebugType>portable</DebugType>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(TargetFramework)|$(Platform)'=='Debug|net7.0|AnyCPU'">
    <DebugType>portable</DebugType>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(TargetFramework)|$(Platform)'=='Release|net7.0|AnyCPU'">
    <DebugType>portable</DebugType>
  </PropertyGroup>

	<ItemGroup>
		<None Include="..\..\Images\YAEP.png">
			<Pack>True</Pack>
			<PackagePath></PackagePath>
		</None>
	</ItemGroup>

  <ItemGroup>
    <PackageReference Include="System.Text.Json" Version="8.0.4" />
  </ItemGroup>

</Project>
