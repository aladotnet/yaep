<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <Version>0.4.0.0</Version>
    <PackageVersion>0.4.0.0</PackageVersion>
    <Description>YAEP Yet another extensions package is a collection of extension methods</Description>
    <Authors>Ahmed Layachi</Authors>
    <Company></Company>
    <Copyright>Copyright 2020-2021 Ahmed Layachi</Copyright>
    <RepositoryUrl>https://github.com/aladotnet/yaep</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <OutputType>Library</OutputType>
    <IsPackable>true</IsPackable>
    <LangVersion>8.0</LangVersion>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <PackageId>YAEP</PackageId>
    <ApplicationIcon>YAEP.ico</ApplicationIcon>
    <PackageProjectUrl>https://github.com/aladotnet/yaep</PackageProjectUrl>
    <PackageIcon>YAEP.png</PackageIcon>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageTags>c#;dotnet;extensions;utils</PackageTags>    
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\..\Images\YAEP.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
